<h1>Hey, UAlbany</h1>
<h4>Find what you need</h4>

<br>

<%= form_tag listings_path, method: :get do %>
  <p>
  <%= text_field_tag :search, params[:search] %>
  <%= submit_tag 'Search', name: nil, class: 'listingSubmitButton'  %>
  </p>
<% end %>

<% @categories.each do |category| %>
  <div class="categoryButton center-block">
    <%= link_to category.name, category %>
  </div>
<% end %>
